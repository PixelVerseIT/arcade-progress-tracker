{
  "manifest_version": 2,
  "name": "Arcade Progress Tracker",
  "version": "1.0.0",
  "description": "moo",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon128.png"
  }
}
